{
    "name": "ekandreas/bladerunner",
    "type": "wordpress-plugin",
    "version": "1.7-beta.1",
    "description": "WordPress Blade L5 template engine",
    "keywords": ["laravel", "blade", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Ek",
            "email": "andreas@elseif.se"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/config": "5.*",
        "illuminate/view": "5.*",
        "hassankhan/config": "0.*",
        "symfony/yaml": "3.*",
        "brain/hierarchy":"2.*"
    },
    "autoload": {
        "psr-4": {
            "Bladerunner\\Tests\\": "tests/",
            "Bladerunner\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "frozzare/wp-test-suite": "*"
    }
}
